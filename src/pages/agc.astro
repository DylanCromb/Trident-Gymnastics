---
import ProgramPage from "../components/ProgramPage.astro";

const title = "AGC";
const blurb = "Australian Gymnastics Club competitive stream—structured training with clear progressions.";
const bookingUrl = "https://YOUR-BOOKING-LINK/agc";
const trialUrl = "https://YOUR-BOOKING-LINK/free-trial";
const levels = [
  { name: "AGC Prep", description: "Foundation strength, shapes and discipline for competition readiness." },
  { name: "AGC Level 1–3", description: "Compulsory skills on beam, bars, floor and vault." },
  { name: "AGC Level 4+", description: "Advanced routines, conditioning and performance mindset." }
];
---

<ProgramPage {title} {blurb} {levels} {bookingUrl} {trialUrl} />
