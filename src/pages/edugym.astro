---
import ProgramPage from "../components/ProgramPage.astro";

const title = "EduGym";
const blurb = "Curriculum-aligned gymnastic movement designed for school groups and physical literacy.";
const bookingUrl = "https://YOUR-BOOKING-LINK/edugym";
const trialUrl = "https://YOUR-BOOKING-LINK/free-trial";
const diagramImage = "/images/levels/edugym-levels.svg";
const levels = [
  { name: "Foundation", description: "Core shapes, rolls and landings building safe foundation skills." },
  { name: "Level 1", description: "Introduction to basic apparatus skills and movement patterns." },
  { name: "Level 2", description: "Develop strength and technique across apparatus with progressions." },
  { name: "Level 3", description: "Intermediate skills and combinations on all apparatus." },
  { name: "Level 4", description: "Advanced skills and complex movement sequences." },
  { name: "Level 5", description: "Refine skills, sequences and confidence for performance and competitions." }
];
---

<ProgramPage {title} {blurb} {levels} {bookingUrl} {trialUrl} diagramImage={diagramImage} />
